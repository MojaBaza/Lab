#include <iostream>
#include <sstream>
#include <vector>
#include <string>
using namespace std;

int main() 
{
    int i, w, r, p, k;
    cout << "Enter i: ";
    cin >> i;
    cout << "Enter w: ";
    cin >> w;
    cout << "Enter r: ";
    cin >> r;
    cout << "Enter p: ";
    cin >> p;
    cout << "Enter k: ";
    cin >> k;
    cin.ignore();

    vector<vector<int>> population;
    cout << "Enter inviduals:" << endl;
    for (int idx = 0; idx < i; ++idx) {
        string line;
        getline(cin, line);
        stringstream ss(line);
        vector<int> chromosome;
        int gene;
        while (ss >> gene) {
            chromosome.push_back(gene);
        }
        population.push_back(chromosome);
    }

    for (size_t idx = 0; idx < population.size(); ++idx) {
        cout << "Invidual " << idx + 1 << ": ";
        for (int gene : population[idx]) cout << gene << " ";
        cout << endl;
    }
    return 0;
}
